        "port": 53,
        "domains": [
          "geosite:cn",
          "domain:ntp.org"
        ]
      }
    ]
  },
  "routing": {
    "domainStrategy": "IPOnDemand",
    "rules": [
      {
        "type": "field",
        "inboundTag": [
          "transparent"
        ],
        "port": 53,
        "outboundTag": "dns-out"
      },
      {
